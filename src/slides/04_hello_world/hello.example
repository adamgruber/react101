<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
  </head>

  <body>
    <!-- Will be replaced by React -->
    <div id="root"></div>

    <!-- Load React -->
    <script src="./react.min.js"></script>
    <script src="./react-dom.min.js"></script> 
    
    <!-- Render element -->
    <script>
      ReactDOM.render(
        React.createElement(
          'h1', null, 'Hello, world!'
        ),
        document.getElementById('root')
      )
    </script>

  </body>

</html>
