const TickTock = props => {
  <div>
    <h1>Hello, world!</h1>
    <h2>The time is now {props.time}.</h2>
  </div>
};

function tick() {
  const now = 
    new Date().toLocaleTimeString();

  ReactDOM.render(
    <TickTock time={now} />,
    document.getElementById('root')
  )  
}

setInterval(tick, 1000);